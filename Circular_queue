#include <stdio.h>
#include <stdlib.h>

#define MAX 5  // Size of the queue

int queue[MAX];
int front = -1;
int rear = -1;

// Function to check if the queue is full
int isFull() {
    if ((rear + 1) % MAX == front) {
        return 1;
    }
    return 0;
}

// Function to check if the queue is empty
int isEmpty() {
    if (front == -1) {
        return 1;
    }
    return 0;
}

// Enqueue (Insert)
void enqueue(int element) {
    if (isFull()) {
        printf("\nQueue is Full! Cannot insert %d\n", element);
    } else {
        if (front == -1) {
            front = 0; // If inserting first element, set front to 0
        }
        rear = (rear + 1) % MAX; // Circular increment
        queue[rear] = element;
        printf("Inserted %d\n", element);
    }
}

// Dequeue (Delete)
void dequeue() {
    if (isEmpty()) {
        printf("\nQueue is Empty! Cannot delete\n");
    } else {
        printf("Deleted %d\n", queue[front]);
        
        // If there was only one element, reset the queue
        if (front == rear) {
            front = -1;
            rear = -1;
        } else {
            front = (front + 1) % MAX; // Circular increment
        }
    }
}

// Display
void display() {
    if (isEmpty()) {
        printf("Queue is Empty\n");
        return;
    }
    
    printf("Queue elements: ");
    int i = front;
    while (i != rear) {
        printf("%d ", queue[i]);
        i = (i + 1) % MAX;
    }
    printf("%d\n", queue[rear]); // Print the last element separately
}

int main() {
    enqueue(10);
    enqueue(20);
    enqueue(30);
    enqueue(40);
    enqueue(50); // Queue is now full (Size 5)
    
    display();
    
    dequeue(); // Removes 10. Front moves to index 1.
    dequeue(); // Removes 20. Front moves to index 2.
    
    display();
    
    enqueue(60); // Works! Goes to index 0 (Circularly)
    enqueue(70); // Works! Goes to index 1
    
    display();
    
    return 0;
}
