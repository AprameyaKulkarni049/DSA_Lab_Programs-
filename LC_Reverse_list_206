/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     struct ListNode *next;
 * };
 */
struct ListNode* reverseList(struct ListNode* head)
{
    struct ListNode *curr=head,*prev=NULL,*second=NULL;
    while(curr!=NULL)
    {
        second=curr->next;
        curr->next=prev;
        prev=curr;
        curr=second;
    }
    return prev;
}
